<%= simple_form_for(@item) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :desc %>
    <%= f.input :price %>
    <%= f.input :found %>
    <%= f.input :category %>
    <%= f.input :image1 %>
    <%= f.input :image2 %>
    <%= f.input :image3 %>
    <%= f.input :user_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
